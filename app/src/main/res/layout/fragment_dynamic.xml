<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dynamicFragId"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".DynamicFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:gravity="center"
        android:text="Rate the Movie :"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/ratingBar4"
        app:layout_constraintEnd_toStartOf="@+id/ratingBar4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49" />

    <RatingBar
        android:id="@+id/ratingBar4"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:outlineAmbientShadowColor="#673AB7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>